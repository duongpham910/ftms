<div class="box box-primary">
  <div class="box-header with-border">
    <h3 class="label box-title"><%= t "document.name" %></h3>
  </div>
  <div class="box-body no-padding">
    <ul class="doc-field">
      <% documents.each do |doc| %>
        <li>
          <%= link_to doc.document_link.url, target: "_blank" do %>
            <abbr title="<%= doc.description %>"><%= doc.title %></abbr>
          <% end %>
        </li>
      <% end %>
    </ul>
  </div>
</div>
