<div id="ranks" class="evaluation-active">
  <div class="row">
    <div class="col-md-12 title">
      <span>
        <%= link_to t("buttons.new"),
          new_trainer_rank_path, class: "btn btn-success pull-right" %>
      </span>
    </div>
  </div>
  <table id="rank-tbl" class="table table-bordered display" data-source="<%= trainer_ranks_url(format: "json") %>" >
    <thead>
      <tr class="active">
        <th></th>
        <th class="rank"><%= t "table.thead.rank" %></th>
        <th class="begin-point"><%= t "table.thead.begin_point" %></th>
        <th class="end-point"><%= t "table.thead.end_point" %></th>
        <th class="action"></th>
        <th class="action"></th>
      </tr>
    </thead>
    <tbody>
    </tbody>
  </table>
</div>
