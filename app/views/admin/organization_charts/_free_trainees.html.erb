      <%= image_tag "user.png", size: Settings.image_size_40 %>
      <span class="title-name"><%= t "organization_charts.free_trainee" %></span>
    </div>
    <div>
      <% profiles.each do |profile| %>
        <%= render "trainee", trainee: profile.user  %>
      <% end %>
    </div>
  </div>
</div>
