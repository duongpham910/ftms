<% if @type == "select_range_time" %>
  $(".filter-form").html('<%= j render "shared/filter/range_date" %>');
  toggleFilterMenu($('.open-select[data-month="<%= @month %>"]'), false);
  loadRangeSelected();
  paintMonths();
<% else %>
  <% if @month.present? %>
    <% if ["evaluation_rank_value", "ratio", "project_name", "payment", "billing",
      "discount", "difference", "price"].include? @type %>
      $(".filter-form").html('<%= j render "shared/filter/form_month",
        name: @type,
        objects: @resources,
        month: @month,
        key_field: :id,
        value_field: :name,
        searchable: true,
        sortable: true %>');
    <% end %>
  <% else %>
    $(".filter-form").html('<%= j render "shared/filter_form",
      name: @type,
      objects: @resources,
      key_field: @key_field,
      value_field: @value_field,
      searchable: true,
      sortable: true %>');
  <% end %>

  intSearcher();
  show_blank_option();
  toggleFilterMenu($('.open-select[data-name="f-<%= @filter_field %>"]'), false);
<% end %>
