<div class="search-user-evaluation">
  <%= search_form_for @search, url: admin_evaluations_path do |f| %>
    <div class="row">
      <div class="col-md-1">
        <%= f.label :name_count, t("seach_user.name_contain") %>
      </div>
      <div class="col-md-4">
        <%= f.search_field :name_cont, class: "form-control" %>
      </div>
      <%= f.submit t("buttons.search"), class: "btn btn-default" %>
    </div>
  <% end %>
</div>
