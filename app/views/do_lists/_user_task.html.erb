<div class="div_user_task">
  <% @to_do_lists.each do |user_task| %>
    <% if user_task.in_progress? && user_task.user_subject.end_date &&
        user_task.user_subject.end_date > Time.now %>
      <div class="dolist">
        <%= link_to user_task_path(user_task), remote: true do %>
          <div class="task">
            <h5><%= t "tasks.task_name", task: user_task.task.name %></h5>
          </div>
          <div class="space"></div>
        <% end %>
      </div>
    <% end %>
  <% end %>
</div>
